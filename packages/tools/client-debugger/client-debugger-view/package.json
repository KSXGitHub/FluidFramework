{
  "name": "@fluid-tools/client-debugger-view",
  "version": "2.0.0-internal.3.0.0",
  "private": true,
  "description": "Contains an extensible debug visualizer / editor for the Fluid client",
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "packages/tools/client-debugger/client-debugger-view"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint && npm run build:docs",
    "build:compile": "npm run build:tsc && npm run build:copy-resources",
    "build:copy-resources": "copyfiles \"src/**/*.css\" dist --up 1",
    "build:docs": "api-extractor run --local && copyfiles -u 1 ./_api-extractor-temp/doc-models/* ../../../../_api-extractor-temp/",
    "build:full": "concurrently npm:build npm:webpack",
    "build:full:compile": "concurrently npm:build:compile npm:webpack",
    "build:genver": "gen-version",
    "build:tsc": "npm run tsc",
    "ci:build:docs": "api-extractor run && copyfiles -u 1 ./_api-extractor-temp/doc-models/* ../../../../_api-extractor-temp/",
    "clean": "rimraf _api-extractor-temp coverage dist nyc *.tsbuildinfo *.build.log",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "format": "npm run prettier:fix",
    "format-and-build": "npm run format && npm run build",
    "format-and-compile": "npm run format && npm run build:compile",
    "lint": "npm run prettier && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start:test-app": "start-server-and-test start:tinylicious 7070 start:test-app:client",
    "start:test-app:client": "webpack serve --config webpack.config.js",
    "start:tinylicious": "tinylicious",
    "test": "npm run test:jest",
    "test:coverage": "npm run test:jest:coverage",
    "test:jest": "jest --detectOpenHandles",
    "test:jest:coverage": "jest --detectOpenHandles --coverage --ci --reporters=default --reporters=jest-junite",
    "tsc": "tsc",
    "webpack": "webpack --env production",
    "webpack:dev": "webpack --env development"
  },
  "dependencies": {
    "@fluentui/react": "^8.104.7",
    "@fluentui/react-hooks": "^8.6.15",
    "@fluentui/react-icons-mdl2": "^1.3.29",
    "@fluentui/react-tabs": "^9.1.9",
    "@fluid-experimental/react-inputs": "workspace:2.0.0-internal.3.0.0",
    "@fluid-tools/client-debugger": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/container-definitions": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/container-loader": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/core-interfaces": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/counter": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/datastore": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/driver-definitions": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/map": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/protocol-definitions": "^1.1.0",
    "@fluidframework/sequence": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/shared-object-base": "workspace:2.0.0-internal.3.0.0",
    "re-resizable": "6.9.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3"
  },
  "devDependencies": {
    "@fluid-example/collaborative-textarea": "workspace:2.0.0-internal.3.0.0",
    "@fluid-tools/webpack-fluid-loader": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/build-common": "^1.1.0",
    "@fluidframework/build-tools": "^0.8.0",
    "@fluidframework/eslint-config-fluid": "^2.0.0",
    "@fluidframework/fluid-static": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/test-runtime-utils": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/tinylicious-client": "workspace:2.0.0-internal.3.0.0",
    "@microsoft/api-extractor": "^7.34.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "22.2.3",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.18",
    "@types/testing-library__jest-dom": "^5.14.5",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "~8.6.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^26.6.3",
    "jest-junit": "^10.0.0",
    "prettier": "~2.6.2",
    "rimraf": "^2.7.1",
    "start-server-and-test": "^1.15.3",
    "style-loader": "^1.3.0",
    "tinylicious": "0.5.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "typescript": "~4.5.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "~4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "nyc",
    "outputName": "jest-junit-report.xml"
  }
}
