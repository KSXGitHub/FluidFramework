{
  "name": "@fluid-example/example-utils",
  "version": "2.0.0-internal.3.0.0",
  "private": true,
  "description": "Shared utilities used by examples.",
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "examples/utils/example-utils"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:docs": "api-extractor run --local --typescript-compiler-folder ../../../node_modules/typescript && copyfiles -u 1 ./_api-extractor-temp/doc-models/* ../../../_api-extractor-temp/",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "ci:build:docs": "api-extractor run --typescript-compiler-folder ../../../node_modules/typescript && copyfiles -u 1 ./_api-extractor-temp/* ../../../_api-extractor-temp/",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix --fix-type problem,suggestion,layout",
    "format": "npm run prettier:fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "prettier": "prettier --check . --ignore-path ../../../.prettierignore",
    "prettier:fix": "prettier --write . --ignore-path ../../../.prettierignore",
    "tsc": "tsc"
  },
  "dependencies": {
    "@fluid-experimental/pact-map": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/aqueduct": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/common-definitions": "^0.20.1",
    "@fluidframework/common-utils": "^1.0.0",
    "@fluidframework/container-definitions": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/container-loader": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/container-runtime": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/container-runtime-definitions": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/core-interfaces": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/driver-definitions": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/driver-utils": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/local-driver": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/register-collection": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/request-handler": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/routerlicious-driver": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/runtime-definitions": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/runtime-utils": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/server-local-server": "^0.1038.2000",
    "@fluidframework/task-manager": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/tinylicious-driver": "workspace:2.0.0-internal.3.0.0",
    "@fluidframework/view-adapters": "workspace:2.0.0-internal.3.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@fluid-tools/build-cli": "^0.8.0",
    "@fluidframework/build-common": "^1.1.0",
    "@fluidframework/eslint-config-fluid": "^2.0.0",
    "@microsoft/api-extractor": "^7.34.0",
    "@rushstack/eslint-config": "^2.6.2",
    "@types/node": "^14.18.36",
    "@types/uuid": "^8.3.4",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "eslint": "~8.6.0",
    "prettier": "~2.6.2",
    "rimraf": "^2.7.1",
    "typescript": "~4.5.5"
  }
}
